{
  "name": "exercise",
  "version": "0.0.0",
  "scripts": {
    "start": "npm-run-all --parallel serve serve:api",
    "serve": "ng serve -o",
    "serve:api": "json-server --watch db.json --delay 500 --port 4300",
    "build": "ng build --configuration production",
    "jest": "jest --config projects/todo-app/jest.config.js",
    "test": "npm run format:test && ng lint && npm run jest",
    "watch": "jest --watch --config projects/todo-app/jest.config.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:write": "prettier projects/**/*.{js,ts,json,html,scss,md} --write",
    "format:test": "prettier projects/**/*.{js,ts,json,html,scss,md} --list-different",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@ngrx/effects": "^15.4.0",
    "@ngrx/entity": "^15.4.0",
    "@ngrx/router-store": "^15.4.0",
    "@ngrx/store": "^15.4.0",
    "@ngrx/store-devtools": "^15.4.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.0.0",
    "uuid": "^3.3.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.9",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.9",
    "@angular/compiler-cli": "^15.2.9",
    "@angular/language-service": "^15.2.9",
    "@ngrx/schematics": "^15.4.0",
    "@types/jest": "^28.1.8",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "codelyzer": "^6.0.0",
    "eslint": "^8.28.0",
    "eslint-plugin-ngrx": "^2.0.0",
    "jest": "^28.1.3",
    "jest-createspyobj": "^1.2.2",
    "jest-preset-angular": "^12.2.6",
    "json-server": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "ts-node": "~7.0.0",
    "typescript": "~4.8.4"
  }
}
